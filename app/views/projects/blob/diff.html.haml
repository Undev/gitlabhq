- if @lines.present?
  - if @form.unfold? && @form.since != 1 && !@form.bottom?
    %tr.line_holder{ id: @form.since }
      %td.old_line.diff-line-num.unfold.js-unfold{data: {linenumber: @form.since}}= "..."
      %td.new_line.diff-line-num.unfold.js-unfold{data: {linenumber: @form.since}}= "..."
      %td.line_content.matched= @match_line

  - @lines.each_with_index do |line, index|
    - line_new = index + @form.since
    - line_old = line_new - @form.offset
    %tr.line_holder
      %td.old_line.diff-line-num{data: {linenumber: line_old}}
        = link_to raw(line_old), "#"
      %td.new_line= link_to raw(line_new) , "#"
      %td.line_content.noteable_line= raw(line)

  - if @form.unfold? && @form.bottom? && @form.to < @blob.loc
    %tr.line_holder{ id: @form.to }
      %td.old_line.diff-line-num.unfold.js-unfold.js-unfold-bottom{data: {linenumber: @form.to}}= "..."
      %td.new_line.diff-line-num.unfold.js-unfold.js-unfold-bottom{data: {linenumber: @form.to}}= "..."
      %td.line_content.matched= @match_line
